Config { 
       font = "xft:WenQuanYi Micro Hei:bold:size=10:antialias=True"
       , bgColor = "black"
       , fgColor = "grey"
       , position = BottomW L 90
       , lowerOnStart = True
       , commands = [ Run Battery ["-t","<acstatus>:<left>%(<timeleft>)"] 60 
		    , Run DiskIO [("/", "R:<read> W:<write>")] [] 5
                    , Run Network "enp0s25" ["-t","<dev>:<rx>","-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Network "wlp3s0" ["-t","<dev>:<rx>","-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem:<usedratio>%"] 10
    		    , Run Com "kanjioftheday" [] "kanji" 60
    		    , Run Com "timeinparis" [] "parisdate" 10
    		    , Run Com "timeinsf" [] "sfdate" 10
    		    , Run Date "%b %_d %H:%M:%S" "date" 10
                    , Run StdinReader
		    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template =
       "%StdinReader%}{%diskio%|%cpu%|%memory%|%eno1%|%wlp3s0%|%battery%|<fc=#ee9a00>SF:%sfdate%</fc>|<fc=#ee9a00>FR:%parisdate%</fc>|%kanji%|<fc=#ee9a00>%date%</fc>"
       }
